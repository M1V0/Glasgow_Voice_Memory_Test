        <Param val="// Initialize sound sequence&amp;#10;soundIndex = 0;      // index of the current sound&amp;#10;repeatIndex = 0;     // how many times current sound has played&amp;#10;sounds = [vm_sound_1, vm_sound_2, vm_sound_3, vm_sound_4,&amp;#10;          vm_sound_5, vm_sound_6, vm_sound_7, vm_sound_8];&amp;#10;currentSound = null; // currently playing sound&amp;#10;&amp;#10;vm_text.setText(vm_text_content);&amp;#10;vm_btn.setText(vm_btn_content);&amp;#10;vm_text.setAutoDraw(true);&amp;#10;vm_btn.setAutoDraw(true);&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
